<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>Bonsai - LabJack Library</Title>
    <Description>Bonsai Library containing interfaces for measurement and automation using LabJack hardware.</Description>
    <PackageTags>Bonsai Rx LabJack</PackageTags>
    <TargetFramework>net462</TargetFramework>
    <Version>0.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bonsai.Core" Version="2.5.0" />
    <PackageReference Include="OpenCV.Net" Version="3.3.1" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="..\Externals\LabJack.LJM.dll" PackagePath="lib\net462" />
    <Reference Include="LabJack.LJM">
      <HintPath>..\Externals\LabJack.LJM.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
